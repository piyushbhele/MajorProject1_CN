<link rel="stylesheet" href="/css/header.css">

<header>
    <h6>Page Header</h6>

    <ul>
        <% if (locals.user){ %>
            <li>
                <a href="/profile/<%= user.id %>">
                    <%= user.name %>
                </a>
            </li>
            <li>
                <a href="/sign-out">Sign Out</a>
            </li>
            <% }else{ %>

                <li>
                    <a href="/sign-in">Sign In</a>
                </li>

                <li>
                    <a href="/sign-up">Sign Up</a>
                </li>


                <% } %>
    </ul>


</header>